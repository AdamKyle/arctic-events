{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "comma-dangle": 2,
    "no-console": 1,
    "no-empty": 2,
    "valid-jsdoc": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-extra-semi": 2,
    "no-unreachable": 2,
    "valid-typeof": 2,
    "complexity": 2,
    "eqeqeq": 2,
    "no-empty-function": 1,
    "max-lines": [2, {"max": 180}],
    "max-nested-callbacks": [2, {"max": 2}],
    "max-params": [2, {"max": 4}]
  }
}
